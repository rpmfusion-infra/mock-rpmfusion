Name:           mock-kwizart
Version:        @VERSION@
Release:        1%{?dist}
Summary:        Mock config files for the rpms.kwizart.net Repository

Group:          Development/Tools
License:        BSD
URL:            http://rpms.kwizart.net/
Source0:        %{name}-%{version}.tar.bz2
BuildRoot:      %{_tmppath}/%{name}-%{version}-%{release}-root-%(%{__id_u} -n)

BuildArch:      noarch
Requires:       mock

%description
Mock config files for the rpms.kwizart.net Repository


%prep
%setup -q -c


%build
#Nothing to build


%install
rm -rf $RPM_BUILD_ROOT
mkdir -p $RPM_BUILD_ROOT%{_sysconfdir}/mock
install -pm 0644 "*kwizart*.cfg" $RPM_BUILD_ROOT%{_sysconfdir}/mock


%clean
rm -rf $RPM_BUILD_ROOT


%files
%defattr(-,root,root,-)
%config(noreplace) %{_sysconfdir}/mock/*kwizart*.cfg


%changelog
